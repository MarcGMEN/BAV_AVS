<?php
require "../Functions/liens_functions.php";

function return_lien_ajax($id) {
  $lien = return_lien($id);
  $lien['lie_dossier']=utf8Encode($lien['lie_dossier']);
  $lien['lie_texte']=utf8Encode($lien['lie_texte']);
  $lien['lie_url']=utf8Encode($lien['lie_url']);
  $lien['lie_texte_lien']=utf8Encode($lien['lie_texte_lien']);
    
  return $lien;
  
}

function return_choixAvant() {
  return recupEnumToArray(CFG_PREFIXE_TABLE."lien","lie_avant");
}
